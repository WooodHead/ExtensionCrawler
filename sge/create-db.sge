#!/bin/bash

module -s load apps/python/conda 2> /dev/null
source activate mypython35

export PATH=~/bin:$PATH
export LD_LIBRARY_PATH=~/lib:$LD_LIBRARY_PATH

set -o nounset

printenv

"$BASEDIR/ExtensionCrawler/create-db" -t 1 -a "$ARCHIVE" -n $SGE_TASK_ID -N $MAX_SGE_TASK_ID "$BASEDIR/out" 2>&1 | bzip2 > "$BASEDIR/logs/$SGE_TASK_ID.log.bz2"
